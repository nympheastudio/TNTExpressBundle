{
    "name": "winzou/tnt-express-bundle",
    "type": "symfony-bundle",
    "description": "Bundle the tnt-express library",
    "keywords": ["symfony", "bundle", "tnt"],
    "license": "MIT",
    "authors": [
        {
            "name": "Alexandre Bacco",
            "homepage": "http://alex.bacco.fr"
        }
    ],
    "require": {
        "php":                       ">5.3.3",
        "symfony/framework-bundle":  "~2.2",
        "doctrine/doctrine-bundle":  "~1.2",
        "doctrine/orm":              "~2.2,>=2.2.3",
        "gedmo/doctrine-extensions": "~2.3",
        "symfony/validator":         "~2.3",
        "symfony/property-access":   "~2.3",
        "winzou/tnt-express":        "@dev"
    },
    "require-dev": {
        "phpspec/phpspec": "~2.0"
    },
    "autoload": {
        "psr-4": { "winzou\\Bundle\\TNTExpressBundle\\": "" }
    },
    "autoload-dev": {
        "psr-4": { "spec\\": "spec/" }
    },
    "config": {
        "bin-dir": "bin"
    }
}
